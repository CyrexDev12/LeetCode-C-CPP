#include <iostream>
#include <stack>
#include <stdio.h>
#include <string> 
#include <cctype>
#include <algorithm>
using namespace std;

class Solution {
    
    public: 

     int calculate(string s) {
        int num = 0; 
        int sign = 1; 
        char prevOp = '+';
        s.erase(remove(s.begin(), s.end(), ' '), s.end());
        stack<int> Stack; 

        for (int i = 0; i < s.length(); i++) {
            char c = s[i];
            if (isdigit(c)) {
                num = num * 10 + (c - '0');
            }

            if (!isdigit(c) || i == s.length() - 1) {
                if (prevOp == '+') {
                    Stack.push(num);
                } else if (prevOp == '-') {
                    Stack.push(-num);
                } else if (prevOp == '*') {
                   int temp = Stack.top();
                   Stack.pop();
                   Stack.push(temp * num);
                } else if (prevOp == '/') {
                    int temp = Stack.top();
                    Stack.pop();
                    Stack.push(temp / num);
                }

                prevOp = c;
                num = 0; 

            }
        }

        int result = 0; 
        while (!Stack.empty()) {
            int temp = Stack.top();
            result += temp;
            Stack.pop();
        }
        return result; 

    }
    
};



  int main() {
        Solution sol;
        cout << "Enter your desired calculation\n";
        string input = "";
        getline(cin, input); // Used to read the enitre line
        cout << "Result: " << sol.calculate(input);
        return 0; 
    }

   

    
